@using MinStat.AnalizeUI.Models
@model IEnumerable<StatisticDataModel>

@{
    ViewBag.Title = "Index";
}

@using (Html.BeginForm())
{
    <input type="text" id="enterpriseId" name="enterpriseId" value="1" />
    <input type="text" id="federalSubjectId" name="federalSubjectId" value="0" />
    <input type="text" id="federalDistrictId" name="federalDistrictId" value="0" />
    <input type="text" id="subsectorId" name="subsectorId" value="0" />
    <input type="text" id="startDate" name="startDate" value="1900-01-01" />
    <input type="text" id="endDate" name="endDate" value="2012-07-21" />
    <input type="submit" value="show" />
}

@foreach (StatisticDataModel m in Model)
{
    @Html.Partial("StatisticData", m)
}

