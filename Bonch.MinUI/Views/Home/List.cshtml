@model IEnumerable<Bonch.Domain.POCO.StatisticDataItem>
@{
    ViewBag.Title = "List";
}
<div id="content" class="summary">
    <table  class="table table-striped table-bordered table-condensed">
        <tr>
            <th>
            </th>
            <th>
            </th>
            <th>
            </th>
            <th>
            </th>
            <th>
            </th>
            <th>
            </th>
            <th>
            </th>
            <th>
            </th>
            <th>
            </th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Part_1)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Part_2)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Part_3)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Part_4)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Part_5)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Activity)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EducationLevel)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.JobLevel)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Count)
                </td>
            </tr>
        }
    </table>
</div>
